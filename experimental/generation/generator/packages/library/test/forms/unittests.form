{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Quantity": {
            "type": "number",
            "minimum": 1,
            "maximum": 10
        },
        "Length": {
            "type": "integer"
        },
        "Complete": {
            "$ref": "template:boolean.schema"
        },
        "NameArray": {
            "type": "array",
            "items": {
                "$ref": "template:personName.schema"
            }
        },       
        "EnumFruit": {
            "type": "string",
            "enum": [
                "orange",
                "apple",
                "pear",
                "banana"
            ]
        },
        "EnumArrayColor": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "yellow",
                    "red",
                    "blue",
                    "black",
                    "white"
                ]
            }
        },
        "Email": {
            "type": "string",
            "format": "email"
        },
        "URL1": {
            "type": "string",
            "format": "uri"
        },
        "URL2": {
            "type": "string",
            "format": "iri"
        },
        "DateTimeProperty": {
            "type": "string",
            "format": "date-time"
        },
        "DateProperty": {
            "type": "string",
            "format": "date"
        },
        "TimeProperty": {
            "type": "string",
            "format": "time"
        },
        "Name": {
            "$ref": "template:personName.schema",
            "pattern" : "^[a-z]{1,3}$"
        },
        "PhoneNumber": {
            "$ref": "template:phonenumber.schema"
        },
        "KeyphraseProperty": {
            "$ref": "template:keyPhrase.schema"
        },
        "Percentage": {
            "$ref": "template:percentage.schema"
        },
        "Age": {
            "$ref": "template:age.schema"
        },
        "OrdinalV2": {
            "$ref": "template:ordinal.schema"
        },
        "GeographyV2": {
            "$ref": "template:geography.schema"
        },
        "Money": {
            "$ref": "template:money.schema"
        },
        "Temperature": {
            "$ref": "template:temperature.schema"
        },
        "Dimension": {
            "$ref": "template:dimension.schema",
            "$units": "feet"
        },
        "DateTime": {
            "$ref": "template:datetime.schema"
        }
    },
    "required": [
        "Quantity",
        "Length",
        "Complete",
        "NameArray",
        "EnumFruit",
        "EnumArrayColor",
        "Email",
        "URL1",
        "URL2",
        "DateTimeProperty",
        "DateProperty",
        "TimeProperty",
        "Name",
        "PhoneNumber",
        "KeyphraseProperty",
        "Percentage",
        "Age",
        "OrdinalV2",
        "GeographyV2",
        "Money",
        "Temperature",
        "Dimension",
        "DateTime"
    ]
}